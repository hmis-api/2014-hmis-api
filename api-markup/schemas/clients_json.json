        "properties": {
          "PersonalID": {
            "type": "string",
            "minLength": 32,
            "maxLength": 32
          },
          "FirstName": {
           "type": "string",
           "maxLength": 50
          },
          "MiddleName": {
            "type": "string",
            "maxLength": 50
          },
          "LastName": {
            "type": "string",
            "maxLength": 50
          },
          "NameSuffix": {
            "type": "string",
            "maxLength": 50
          },
          "NameDataQuality": {
            "type" : "integer"
          },
          "SSN": {
            "type": "string",
            "maxLength": 9
          },
          "SSNDataQuality": {
            "type" : "integer"
          },
          "DOB": {
            "type": "string",
            "format": "date-time"
          },
          "DOBDataQuality": {
            "type": "integer"
